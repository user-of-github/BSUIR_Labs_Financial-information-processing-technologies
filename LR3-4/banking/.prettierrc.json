{
  "trailingComma": "none",
  "semi": true,
  "tabWidth": 2,
  "singleQuote": true,
  "jsxSingleQuote": false,
  "printWidth": 130,
  "plugins": ["prettier-plugin-tailwindcss", "prettier-plugin-organize-imports", "@trivago/prettier-plugin-sort-imports"],
  "importOrder": ["react", "^next/(.*)$", "<THIRD_PARTY_MODULES>", "^@core/(.*)$", "^@components/(.*)$", "^[./]"],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
